var weight = prompt("Please enter your weight (in kg's) : ")
var height = prompt("Please enter your height (in meters) : ")

var bmi = (weight / (height * height));

if (bmi < 18.5) { ;
    alert("your bmi is " + bmi + " ,so you are under weight");
}

if (bmi > 18.5 && bmi < 24.9) {;
    alert ("your bmi is " + bmi + " ,so you have normal weight");
}
if (bmi > 24.9) {;
    alert ("your bmi is " + bmi + " ,so you are over weighted");
}

